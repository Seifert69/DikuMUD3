dilcopy quest_restrict@function(XMAS_DONE20,0,0,"");

dilbegin candyheal();
var
	i : integer;
        enemy :unitptr;
        n:integer;

code
{
	heartbeat := PULSE_SEC*5;
:start:
	wait(SFB_COM,(activator.type ==UNIT_ST_PC)and (activator==self.outside) and
	(command(CMD_AUTO_COMBAT)) and (self.equip == WEAR_WIELD));
	i := rnd (1,10);
	enemy := self.outside.fighting;

    if (enemy.level >IMMORTAL_LEVEL)
      goto start;

	if (i > 2) goto start;
	n:= cast_spell (SPL_CURE_WOUNDS_2,self.outside,self,self.outside,"candy_heal@holiday20");

	goto start;
}

dilend
end

