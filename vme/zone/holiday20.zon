// Program will add charges to the christmas item.

dilbegin recall x_charge();
var
  exdp : extraptr;
  pc : unitptr;
  charge 	: integer;
code
{
	:start:
	heartbeat := PULSE_SEC*SECS_PER_MUD_DAY;
	wait(SFB_TICK, (self.equip > 0));
	heartbeat := PULSE_SEC*5;
	
	pc:=self.outside;
	secure(pc,lostpc);
	
	if(not(X_CHARGES in pc.quests))
	{
		addextra(pc.quests, {X_CHARGES}, "");
		exdp := X_CHARGES in pc.quests;
		addstring(exdp.names, "charged");
	}
	else
	{	
		exdp := X_CHARGES in pc.quests;
	}

	if(exdp.names.[1] == "empty")
	{
		act("Your $2n twinkles magically!",A_ALWAYS,self.outside,self,null,TO_CHAR);
		act("$1n's $2n twinkles magically!",A_ALWAYS,self.outside,self,null,TO_REST);
		self.extra.descr := self.extra.descr + " It has a <div class='cpg'>magical</div><div class='cpw'> twinkle</div>.";
		substring(exdp.names, "empty");
		addstring(exdp.names, "charged");
