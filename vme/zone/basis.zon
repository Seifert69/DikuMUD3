dilbegin fixwt ();
external
 integer fixweight(u:unitptr);
 integer carry_w_limit@function(u:unitptr);
var
 item:unitptr;
 wgt:integer;

code
{
heartbeat := PULSE_SEC;
fixweight(self);
wgt:=self.baseweight+carry_w_limit@function(self);
