dilbegin destroy_money();
var
        pc : unitptr;
        item : unitptr;
        list : extraptr;
code
{
heartbeat := PULSE_SEC*3;
:start:

wait(SFB_DONE, (command("give") and (target == self) and
      (activator.type == UNIT_ST_PC) ));

:give:
priority;
item := medium;
pc := activator;
secure(pc, losthim);
secure(item,losthim);
exec("emote counts the money." ,self);
destroy(item);
unsecure(pc);
:losthim:
goto start;
}
dilend

dilbegin appocalypse();
external
   magic@basis(u1 : unitptr);
   doom@basis (u1 : unitptr);
var
  u1 : unitptr;
code
{
   :loop:
   heartbeat := PULSE_SEC;
   wait(SFB_CMD, command("doom") and (activator.level >= 254));
   block;
   exec("nod", self);
   pause;

   heartbeat := PULSE_SEC * 5;
   u1 := findrndunit(self, FIND_UNIT_WORLD, UNIT_ST_NPC);
   if ((u1.idx == self.idx) or (u1 == null))
   {
      exec("shrug", self);
      pause;
      goto loop;
   }
   if (u1 == null)
      log("X: u1 is NULL");

   exec("grin", self);

   if (rnd(0,1))
     magic@basis(u1);
   else if (rnd(0,1))
     doom@basis(u1);
   pause;
   goto loop;
}
dilend



