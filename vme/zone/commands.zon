 }
else if (t_obj.value[0]==-1)
 amount:=f_obj.value[0];
else

 {
 amount:=t_obj.capacity-t_obj.value[0];
 if (f_obj.value[0]<amount)
  amount:=f_obj.value[0];
 if (amount <0)
