 quit;
 }

if ((f_obj.type!=UNIT_ST_OBJ) or (f_obj.objecttype!=ITEM_DRINKCON))
 {
 act("You can not pour from that.",A_SOMEONE, self, null, null, TO_CHAR);
 quit;
 }

if (f_obj.value[0]<=0)
 {
