 act("$1n empties $2n.",A_HIDEINV, self, f_obj, null, TO_ROOM);
 act("You empty $2n.",A_SOMEONE, self, f_obj, null, TO_CHAR);

 sub_drink_info(f_obj);
send_done ("pour",self,f_obj,self.outside,0,"",null);
 quit;
 }


 :pour_in:
