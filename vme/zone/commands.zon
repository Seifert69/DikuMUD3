dilbegin stringlist  add_in_order (sl:stringlist,s:string);
var
 i:integer;
 ln:integer;
sln:integer;

code
{
if (length(sl)==0)
 {
 addstring (sl,s);
 return (sl);
 }

sln:=length(s);
ln:=length(sl);
i:=0;
while (i<ln)
 {
 if (length(sl.[i]) <=sln)
  {
  insert (sl,i,s);
 return(sl);
 }
 i:=i+1;
 }

addstring (sl,s);
return (sl);
}
