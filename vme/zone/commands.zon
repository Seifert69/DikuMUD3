 }

if (t_obj.value[0]>=0)
 {
 t_obj.value[0]:=t_obj.value[0]+amount;
 setweight (t_obj,t_obj.weight+amount);
 }

  /* Then the poison boogie */
if (f_obj.value[3]>t_obj.value[3])
t_obj.value[3]:=rnd(f_obj.value[3],t_obj.value[3]);

if (f_obj.value[0]==0)
 {
 sub_drink_info(f_obj);
 }

