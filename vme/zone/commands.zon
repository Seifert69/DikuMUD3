dilbegin stringlist shell(s:stringlist);
var
   ln:integer;
   imax:integer;
   i:integer;
   iVal:string;
   inc:integer;
   j:integer;
code
{
   iMax:=length(s);
   inc:=0;

   while (inc < iMax)
      inc := inc *3+1;

inc:=inc/3;
while(inc > 0)
 {
 i:=inc;
 while (i < iMax)
  {
  iVal := s.[i];
  j:=i;
  while(j > (inc-1))
   {
   if(iVal < s.[j - inc])
    {
    s.[j] := s.[j - inc];
    }
   else
    break;
   j:=j-inc;
   }
  s.[j] := iVal;
  i:=i+1;
  }
 inc := inc /3;
 }

return (s);
}
dilend

