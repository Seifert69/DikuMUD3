         while (cnt<ln)
         {
            if (temp==slist.[cnt])
               break;
            cnt:=cnt+1;
         }

         if (cnt!=ln)
         {
            i := atoi(ilist.[cnt]);
            i := i + 1;
            ilist.[cnt] := itoa(i);
            cnt:=cnt+1;
         }
         else
         {
            addstring(slist, temp);
            addstring(clist, temp1);
            cnt := length(slist);
            addstring(ilist, "1");
         }
      } // if visible

      item := item.next;
   }

   cnt := 0;

   if (not length(slist))
      buffer := buffer + "<div class='obj_title'> Nothing.</div><br/>";
   else
   {
      while(cnt < length(slist))
      {
         if (atoi(ilist.[cnt]) > 1)
            temp := clist.[cnt] + "[x" + ilist.[cnt] + "] ";
         else
            temp := clist.[cnt];

         buffer := buffer + temp + slist.[cnt] + "</div><br/>";

         cnt := cnt + 1;
      }
   }

   pagestring(buffer, self);
   quit;
}
