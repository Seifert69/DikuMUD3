      i:=i+1;
      }

         }
         else
         goto not_leader;
         }

   }
   else
   sendtext ("You are not in a group silly.<br/>",self);
   }
   else
   sendtext("Use either 'score' or 'score group'<br/>",self);

   :lead_end:
   unsecure(leader);
   quit;
}
